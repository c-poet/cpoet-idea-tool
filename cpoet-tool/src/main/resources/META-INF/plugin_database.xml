<idea-plugin>
    <actions>
        <action class="cn.cpoet.tool.actions.database.CopySingleColJoinAction">
            <add-to-group group-id="Console.TableResult.PopupGroup" relative-to-action="Console.TableResult.CopyAggregatorResult" anchor="after" />
        </action>

        <group id="CPoetTool_Console.TableResult.CellEditor.Popup" text="CPoet Tool" popup="true" searchable="false">
            <separator/>
            <add-to-group group-id="Console.TableResult.CellEditor.Popup" anchor="after" relative-to-action="EditorToggleColumnMode"/>
            <reference ref="CPoetTool_UUIDGenerateAction" />
            <reference ref="CPoetTool_ConvertToCamelAction" />
            <reference ref="CPoetTool_ConvertToUnderlineAction" />
            <reference ref="CPoetTool_Base64EncodeAction" />
            <reference ref="CPoetTool_Base64DecodeAction" />
            <reference ref="CPoetTool_Md5Action" />
            <reference ref="CPoetTool_Sha1Action" />
        </group>
    </actions>
</idea-plugin>
